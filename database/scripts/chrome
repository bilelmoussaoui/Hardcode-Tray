#!/bin/bash
cd ./database/scripts/
mkdir -p extracted

if [[ $1 == *.svg ]]; then
    python3 svgtopng.py $1 $(echo $2 | cut -f 1 -d '.')
else
    cp $1 $(echo $2 | cut -f 1 -d '.')
fi

./data_pack.py $5 $(echo $2 | cut -f 1 -d '.')

if [ "$4" -eq -1 ]; then
    mv $5 $3/$5
    rm -rf ./extracted
fi
