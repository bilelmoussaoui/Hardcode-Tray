        "tray@2x": {
            "original": "assets/images/tray/linux/tray@2x.png",
            "theme": "ferdium-tray"
        },
        "tray-indirect": {
            "original": "assets/images/tray/linux/tray-indirect.png",
            "theme": "ferdium-indirect"
        },
        "tray-indirect@2x": {
            "original": "assets/images/tray/linux/tray-indirect@2x.png",
            "theme": "ferdium-indirect"
        },
        "tray-unread": {
            "original": "assets/images/tray/linux/tray-unread.png",
            "theme": "ferdium-unread"
        },
        "tray-unread@2x": {
            "original": "assets/images/tray/linux/tray-unread@2x.png",
            "theme": "ferdium-unread"
