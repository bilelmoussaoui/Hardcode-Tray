{
    "name": "Yakyak",
    "app_path": [
        "/usr/share/yakyak/",
        "/opt/yakyak/"
    ],
    "icons_path": [
        "/usr/share/yakyak/app/icons/",
        "/opt/yakyak/resources/app/icons/"
    ],
    "icons": {
        "indicator": {
            "original": "icon-read.png",
            "theme": "hangouts-available"
        },
        "indicator@2x": {
            "original": "icon-read@2x.png",
            "theme": "hangouts-available"
        },
        "indicator@8x": {
            "original": "icon-read@8x.png",
            "theme": "hangouts-available"
        },
        "indicator@20": {
            "original": "icon-read@20.png",
            "theme": "hangouts-available"
        },
        "notification": {
            "original": "icon-unread.png",
            "theme": "hangouts-notification",
            "symlinks": [
                "icon.png"
            ]
        },
        "notification@2x": {
            "original": "icon-unread@2x.png",
            "theme": "hangouts-notification",
            "symlinks": [
                "icon@2x.png"
            ]
        },
	    "notification@8x": {
            "original": "icon-unread@8x.png",
            "theme": "hangouts-notification",
            "symlinks": [
                "icon@8x.png"
            ]
        },
	    "notification@20": {
            "original": "icon-unread@20.png",
            "theme": "hangouts-notification",
            "symlinks": [
                "icon@20.png"
            ]
        }
    }
}
